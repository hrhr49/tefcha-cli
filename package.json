{
  "name": "tefcha-cli",
  "version": "0.0.14",
  "description": "A CLI Tool to Convert Text to Flowchart.",
  "bin": {
    "tefcha": "bin/cli.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "bin/"
  ],
  "scripts": {
    "copy:js": "cross-env cp node_modules/tefcha/umd/tefcha.js bin/",
    "copy:font": "cross-env cp node_modules/noto-fontface-cjk-jp/fonts/Noto/NotoSansCJKjp-Regular.otf bin/",
    "copy:html": "cross-env cp html/index.html bin/",
    "prepublishOnly": "npm-run-all clean copy:* build",
    "clean": "rimraf bin/*",
    "build": "tsc",
    "test:cli": "cross-env NODE_ENV=develop ./test/test.sh",
    "test:jest": "cross-env NODE_ENV=develop jest",
    "test": "npm-run-all clean copy:* build test:*",
    "preinstall": "typesync || :"
  },
  "author": "hrhr49 <hiro49410@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^6.0.0",
    "noto-fontface-cjk-jp": "^1.0.2",
    "opentype.js": "^1.3.3",
    "puppeteer": "^9.1.1",
    "sharp": "^0.29.1",
    "tefcha": "^0.0.11"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^27.0.1",
    "@types/opentype.js": "^1.3.3",
    "@types/puppeteer": "^3.0.1",
    "@types/rimraf": "^3.0.2",
    "@types/sharp": "^0.29.2",
    "cross-env": "^7.0.2",
    "file-type": "^16.5.3",
    "jest": "^27.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^3.9.7",
    "typesync": "^0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hrhr49/tefcha-cli.git"
  },
  "bugs": {
    "url": "https://github.com/hrhr49/tefcha-cli/issues"
  },
  "homepage": "https://github.com/hrhr49/tefcha-cli#readme"
}
